extends ../layout

block layout-content
    div#auswertenContainer
        img(src='../icons/Logo.svg').logoImage
        div.navigator#navigatorTop.navAuswerten(style='text-align:center')
            div
                h3#auswertenTitle.fivePadding
                
        div.stepsContainerNoMaxHeight.auswertenScroll
            span#legend Anzahl der Siege (Mehrere Sieger pro Person m√∂glich)
            canvas#summaryChart
            //-span.fivePadding#mehrere 
            //-a#link(download='filename.png') Save as Image
            div
                div.fivePadding
                    h4#bisherige Bisherige Teilnehmer
                    table#participantsTable
                div.fivePadding#aggMatrixContainer
                    span(style="padding-right:5px")#aggToggle +
                    h4#aggSpoiler(onLoad='console.log("d")').noselect Aggregationsmatrix
                    table#aggTable
    span#dada(style="visibility:hidden;z-index: -2;") #{uuid}
    span#xaxa(style="visibility:hidden;z-index: -2;") #{aggMatrixJSON}
    script(src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.6/Chart.bundle.min.js" type="text/javascript")
    script(src="../scripts/auswerten/summary.js" type="module")
    script(src="../scripts/auswerten/summaryData.js")
    script(src="../scripts/poll.js")
    script(src="../scripts/criteria.js")
    script(src="../scripts/main.js")
    
    

    
